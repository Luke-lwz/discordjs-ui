{
  "name": "discordjs-ui",
  "version": "1.0.0",
  "description": "Discord Bot UI made easy",
  "main": "discordjs-ui.min.js",
  "types": "discordjs-ui.d.ts",
  "scripts": {
    "_clear": "rimraf build/compiled/*",
    "_tsc": "tsc",
    "_make-bundle": "node build/make-bundle",
    "build": "run-s _tsc _make-bundle",
