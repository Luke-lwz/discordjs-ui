    "discord",
    "bot",
    "discord-bot"
  ],
  "author": "Lukas Lunkwitz",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.14.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "uglify-js": "^3.17.4"
  }
}
